<p id="notice"><%= notice %></p>

<h1>Fakemodels</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fakemodels.each do |fakemodel| %>
      <tr>
        <td><%= link_to 'Show', fakemodel %></td>
        <td><%= link_to 'Edit', edit_fakemodel_path(fakemodel) %></td>
        <td><%= link_to 'Destroy', fakemodel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fakemodel', new_fakemodel_path %>
